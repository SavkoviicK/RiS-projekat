package com.veterinarska.stanica.dto;

public class LjubimacDTO {
    private Long id;
    private String ime;
    private String vrsta;
    private String rasa;
    private String pol;
    private Long vlasnikId;
    private String vlasnikEmail;
    private String napomena;

    public LjubimacDTO() {}
    public LjubimacDTO(Long id, String ime, String vrsta, String rasa, String pol,
                       Long vlasnikId, String vlasnikEmail) {
        this.id = id; this.ime = ime; this.vrsta = vrsta; this.rasa = rasa; this.pol = pol;
        this.vlasnikId = vlasnikId; this.vlasnikEmail = vlasnikEmail;
    }

    // getteri/setteri
    public Long getId() { return id; }
    public void setId(Long id) { this.id = id; }
    public String getIme() { return ime; }
    public void setIme(String ime) { this.ime = ime; }
    public String getVrsta() { return vrsta; }
    public void setVrsta(String vrsta) { this.vrsta = vrsta; }
    public String getRasa() { return rasa; }
    public void setRasa(String rasa) { this.rasa = rasa; }
    public String getPol() { return pol; }
    public void setPol(String pol) { this.pol = pol; }
    public Long getVlasnikId() { return vlasnikId; }
    public void setVlasnikId(Long vlasnikId) { this.vlasnikId = vlasnikId; }
    public String getVlasnikEmail() { return vlasnikEmail; }
    public void setVlasnikEmail(String vlasnikEmail) { this.vlasnikEmail = vlasnikEmail; }
}
